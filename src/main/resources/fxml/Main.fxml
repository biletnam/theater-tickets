<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="900.0" prefWidth="1200.0" stylesheets="@../styles/Styles.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.sawmill.theater.ticketing.controller.MainController">
   <children>
      <Label layoutX="39.0" layoutY="36.0" text="COOK FOREST SAWMILL CENTER FOR THE ARTS  |" textFill="WHITE">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <Label layoutX="813.0" layoutY="43.0" text="Where nature meets the arts!" textFill="WHITE">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Button fx:id="btnAddShowtime" layoutX="55.0" layoutY="289.0" mnemonicParsing="false" onAction="#showNewShowtime" prefHeight="44.0" prefWidth="291.0" text="Add New Showtime">
        <cursor>
          <Cursor fx:constant="OPEN_HAND" />
        </cursor>
      </Button>
      <Button layoutX="55.0" layoutY="360.0" mnemonicParsing="false" prefHeight="44.0" prefWidth="291.0" text="Remove Showtime">
        <cursor>
          <Cursor fx:constant="OPEN_HAND" />
        </cursor>
      </Button>
      <Button layoutX="55.0" layoutY="428.0" mnemonicParsing="false" onAction="#getShowtimes" prefHeight="44.0" prefWidth="291.0" text="Print Tickets">
        <cursor>
          <Cursor fx:constant="OPEN_HAND" />
        </cursor>
      </Button>
      <Button layoutX="55.0" layoutY="499.0" mnemonicParsing="false" prefHeight="44.0" prefWidth="291.0" text="Reports">
        <cursor>
          <Cursor fx:constant="OPEN_HAND" />
        </cursor>
      </Button>
      <Button layoutX="55.0" layoutY="773.0" mnemonicParsing="false" onMouseClicked="#closeApplication" prefHeight="44.0" prefWidth="291.0" text="Exit">
        <cursor>
          <Cursor fx:constant="OPEN_HAND" />
        </cursor>
      </Button>
   </children>
</AnchorPane>
